        <insert-service>
            <insert-service-id>document_selection_hs</insert-service-id>
            <insert-service-name>module.newsletter.blog.insertService.name</insert-service-name>
            <insert-service-bean-class>fr.paris.lutece.plugins.newsletter.modules.blog.web.NewsletterDocumentServiceJspBean</insert-service-bean-class>
            <insert-service-label>module.newsletter.blog.insertService.label</insert-service-label>
        </insert-service>

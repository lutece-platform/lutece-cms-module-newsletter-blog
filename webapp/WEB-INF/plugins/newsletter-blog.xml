<?xml version="1.0" encoding="UTF-8"?><plug-in>
    
    <!-- Plugin Informations -->
    <name>newsletter-blog</name>
    <class>fr.paris.lutece.plugins.newsletter.modules.blog.service.NewsletterBlogPlugin</class>
    <version>1.0.3</version>
    <description>module.newsletter.blog.module.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider>module.newsletter.blog.module.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>themes/admin/shared/plugins/newsletter/images/modules/blog/newsletter-blog.svg</icon-url>
